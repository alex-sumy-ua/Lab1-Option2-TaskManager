<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.plachkovskyy.aleksandr.tasks.controller.MainController">
   <children>
      <TextArea layoutX="10.0" layoutY="33.0" prefHeight="313.0" prefWidth="593.0" />
      <Button fx:id="calendarButton" layoutX="35.0" layoutY="360.0" mnemonicParsing="false" onAction="#onCalendarButtonPressed" prefHeight="25.0" prefWidth="100.0" text="Calendar" />
      <Label fx:id="modeLabel" layoutX="20.0" layoutY="10.0" text="Using download list" />
      <Button fx:id="addButton" layoutX="318.0" layoutY="360.0" mnemonicParsing="false" onAction="#onAddButtonPressed" prefHeight="25.0" prefWidth="100.0" text="Add new task" />
      <Button fx:id="detailsButton" layoutX="177.0" layoutY="360.0" mnemonicParsing="false" onAction="#onDetailsButtonPressed" prefHeight="25.0" prefWidth="100.0" text="Task details" />
      <Button fx:id="exitButton" layoutX="460.0" layoutY="360.0" mnemonicParsing="false" onAction="#onExitButtonPressed" prefHeight="25.0" prefWidth="100.0" text="Exit" />
   </children>
</AnchorPane>
